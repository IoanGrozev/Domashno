#include <iostream>

int main()
{
    int n;
    std::cin >> n;

    int broi_visokosni = 0;

    for (int i = 1; i <= n; i++)
    {
        int godina;
        std::cin >> godina;
        if ((godina % 4 == 0) && (godina % 100 != 0))
        {
            broi_visokosni++;
        }
       
    }
    std::cout << broi_visokosni;
}


